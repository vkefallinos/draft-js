'use strict';

import type DraftMetaInstance from './DraftMetaInstance';
import type { MetaMap } from './MetaMap';

function addMetaToMetaMap(metaMap: MetaMap, instance: DraftMetaInstance, key: string): MetaMap {
  return metaMap.set(`${key}`, instance);
}

module.exports = addMetaToMetaMap;